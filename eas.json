{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.life-control.com.br"
      }
    },
    "build:apk": {
      "android": {
        "gradleCommand": ":app:assembleRelease --parallel --daemon --max-workers=4"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.life-control.com.br"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.life-control.com.br"
      }
    }
  },
  "submit": {
    "production": {}
  }
}